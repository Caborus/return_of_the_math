{
  "manifest_version": 3,
  "name": "CoolMathGames Redirect Challenge",
  "version": "1.0",
  "description": "Challenge users with a math problem before redirecting to Cool Math Games",
  "permissions": ["activeTab", "webNavigation", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": ["https://www.coolmathgames.com/*"],
        "js": ["content.js"]
    }
    ]
}
